#ifndef GODZIWEBCONTROL_NODEMASKS_H
#define GODZIWEBCONTROL_NODEMASKS_H

namespace GodziWebControl
{
    enum NodeMasks
    {
        TERRAIN     = 0x1 << 0,
        ICONS       = 0x1 << 1,
        TEXT_LABELS = 0x1 << 2,
        MODELS      = 0x1 << 3
    };
}

#endif // GODZIWEBCONTROL_NODEMASKS_H